{
  "name": "@logux/vuex",
  "version": "0.9.22",
  "description": "Vuex compatible API for Logux",
  "keywords": [
    "logux",
    "client",
    "vuex",
    "websocket"
  ],
  "scripts": {
    "test": "jest --coverage && eslint . && check-dts && size-limit && yaspeller *.md"
  },
  "author": "Eduard Aksamitov <e@edaks.co>",
  "license": "MIT",
  "homepage": "https://logux.io/",
  "repository": "logux/vuex",
  "types": "./index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=10.0.0"
  },
  "peerDependencies": {
    "vue": ">=3.0.8",
    "vuex": ">=4.0.0"
  },
  "dependencies": {
    "@logux/client": "^0.9.3",
    "@logux/core": "^0.6.2",
    "nanoevents": "^5.1.13"
  },
  "devDependencies": {
    "@logux/eslint-config": "^45.2.1",
    "@logux/sharec-config": "^0.9.0",
    "@size-limit/dual-publish": "^4.10.1",
    "@size-limit/preset-small-lib": "^4.10.1",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "@vue/test-utils": "^2.0.0-rc.4",
    "check-dts": "^0.4.4",
    "dual-publish": "^1.0.5",
    "eslint": "^7.23.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^1.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^29.0.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "nanodelay": "^1.0.8",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.0.2",
    "size-limit": "^4.10.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3",
    "vue": "^3.0.8",
    "vuex": "^4.0.0",
    "yaspeller": "^7.0.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.md": "yaspeller",
    "*.js": [
      "prettier --write",
      "eslint"
    ],
    "*.ts": [
      "prettier --write",
      "eslint"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "prettier": {
    "arrowParens": "avoid",
    "jsxSingleQuote": false,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "eslintConfig": {
    "extends": "@logux/eslint-config/ts",
    "rules": {
      "@typescript-eslint/no-var-requires": "off",
      "@typescript-eslint/no-explicit-any": "off"
    }
  },
  "eslintIgnore": [
    "**/errors.ts"
  ],
  "size-limit": [
    {
      "name": "Store",
      "import": "{ CrossTabClient, createStoreCreator, useStore }",
      "limit": "10 KB"
    },
    {
      "name": "Store + Helpers",
      "import": "{ CrossTabClient, createStoreCreator, useStore, useSubscription, Subscribe }",
      "limit": "11 KB"
    }
  ],
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "ignoreText": [
      " \\(by [^)]+\\)"
    ],
    "dictionary": [
      "versioning",
      "peerDependencies",
      "npm",
      "vue",
      "GraphQL",
      "io",
      "logux",
      "vuex",
      "mixin",
      "mixinâ€™s",
      "TypeDoc",
      "WebSocket",
      "payload",
      "ES",
      "roadmap",
      "refactor",
      "composable",
      "unsubscribes",
      "nanoevents"
    ]
  },
  "sharec": {
    "config": "@logux/sharec-config",
    "version": "0.9.0"
  }
}
